{
  "disable_ai": true,
  "lsp_highlight_debounce": 0,
  "hover_popover_delay": 0,
  "selection_highlight": false,
  "buffer_line_height": "standard",
  "agent": {
    "button": false,
    "model_parameters": []
  },
  "collaboration_panel": {
    "button": true
  },
  "notification_panel": {
    "button": true
  },
  "git_panel": {
    "button": true
  },
  "outline_panel": {
    "button": true
  },
  "active_pane_modifiers": {},
  "use_system_window_tabs": false,
  "bottom_dock_layout": "contained",
  "preview_tabs": {
    "enabled": false
  },
  "tabs": {
    "file_icons": false,
    "git_status": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show": true
  },
  "title_bar": {
    "show_sign_in": false,
    "show_user_picture": false,
    "show_onboarding_banner": false,
    "show_project_items": true,
    "show_branch_name": true
  },
  "debugger": {
    "button": false
  },
  "search": {
    "button": false
  },
  "terminal": {
    "button": false
  },
  "status_bar": {
    "active_language_button": false,
    "cursor_position_button": true
  },
  "project_panel": {
    "hide_hidden": false,
    "hide_root": true,
    "indent_guides": {
      "show": "always"
    },
    "starts_open": true,
    "auto_fold_dirs": true,
    "show_diagnostics": "off",
    "git_status": true,
    "folder_icons": false,
    "file_icons": false,
    "button": false
  },
  "diagnostics": {
    "button": false,
    "inline": {
      "enabled": true
    }
  },
  "prettier": {
    "allowed": true
  },
  "tasks": {
    "enabled": true
  },
  "jsx_tag_auto_close": {
    "enabled": false
  },
  "use_auto_surround": false,
  "use_autoclose": false,
  "ensure_final_newline_on_save": false,
  "indent_guides": {
    "line_width": 1,
    "enabled": false
  },
  "toolbar": {
    "agent_review": false,
    "selections_menu": false,
    "quick_actions": false,
    "breadcrumbs": true
  },
  "scrollbar": {
    "selected_symbol": false,
    "selected_text": false,
    "show": "never"
  },
  "gutter": {
    "min_line_number_digits": 2,
    "folds": false,
    "breakpoints": false,
    "runnables": false
  },
  "relative_line_numbers": "enabled",
  "autosave": "off",
  "rounded_selection": false,
  "current_line_highlight": "none",
  "unnecessary_code_fade": 0.0,
  "cursor_shape": "block",
  "cursor_blink": false,
  "multi_cursor_modifier": "cmd_or_ctrl",
  "agent_ui_font_size": 16.0,
  "ui_font_weight": 700.0,
  "ui_font_family": "JetBrainsMono Nerd Font Mono",
  "buffer_font_weight": 700.0,
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "on_last_window_closed": "quit_app",
  "when_closing_with_no_tabs": "close_window",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": true,
  "base_keymap": "VSCode",
  "icon_theme": "Zed (Default)",
  "ui_font_size": 15.0,
  "buffer_font_size": 16.0,
  "theme": "Zedokai (Filter Ristretto)"
}
