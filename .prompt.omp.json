{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "{{ .Path }}",
          "foreground": "#6B8F9A",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": " <#A3BE8C>git(</><#D3869B>{{ .HEAD }}</><#A3BE8C>)</>",
          "foreground": "#C0C6C0",
          "type": "git",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
