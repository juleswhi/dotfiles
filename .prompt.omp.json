{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "folder"
          },
          "template": "{{ .Path }}",
          "foreground": "#56B6C2",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "branch_icon": "",
            "cache_duration": "none"
          },
          "template": " <#DDB15F>git(</>{{ .HEAD }}<#DDB15F>)</>",
          "foreground": "#D4AAFC",
          "type": "git",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3,
  "final_space": true
}
